local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Blood Hub | V0.0.1", "BloodTheme")

-- Players

local Player = Window:NewTab("Player")

local Section = Player:NewSection("Mouvement")

Section:NewSlider("Walk Speed", "Walk Speed Slider (you chose number <16 - 500>)", 500, 16, function(s) -- 500 (MaxValue) | 16 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("Jump Power", "Jump Power Slider (you chose number <50 - 500>)", 500, 50, function(s) -- 500 (MaxValue) | 50 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Section:NewToggle("Walk On Walls", "Toggle the buton to function", function(state)
    if state then
        print(loadstring(game:HttpGet("https://pastebin.com/raw/zXk4Rq2r"))())
    else
        print(loadstring(game:HttpGet("https://pastebin.com/raw/zXk4Rq2r"))())
    end
end)

-- Other

local Other = Window:NewTab("Other")

local Section = Other:NewSection("Script")

Section:NewButton("Keyboard Script", "Thx to kokjindesu for the script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end)

-- Settings

local Set = Window:NewTab("Settings")

local Section = Set:NewSection("Blood Hub")

Section:NewKeybind("Toggle UI", "Press E on the keyboard", Enum.KeyCode.E, function()
	Library:ToggleUI()
end)

-- Credits

local Credit = Window:NewTab("Credits")

local Section = Credit:NewSection("Creator")

Section:NewButton("!  >Dialz.#4528", "Discord of Creator Blood Hub (click to copy)", function()
    setclipboard("!  >Dialz.#4528")
end)

local Section = Credit:NewSection("Developper")

Section:NewButton("!  >Dialz.#4528", "Discord of Developper Blood Hub (click to copy)", function()
    setclipboard("!  >Dialz.#4528")
end)

local Section = Credit:NewSection("Scripter")

Section:NewButton("!  >Dialz.#4528", "Discord of Scripter Blood Hub (click to copy)", function()
    setclipboard("!  >Dialz.#4528")
end)

local Section = Credit:NewSection("Disagner")

Section:NewButton("!  >Dialz.#4528", "Discord of Disagner Blood Hub (click to copy)", function()
    setclipboard("!  >Dialz.#4528")
end)

local Section = Credit:NewSection("Discord Server")

Section:NewButton("Discord Link", "Discord Server Blood Hub (click to copy)", function()
    setclipboard("https://discord.gg/J44MErAWrZ")
end)
